  },
  "bin": {
    "fast-install": "bin/index.js"
  },
  "scripts": {
    "build": "yarn clean && yarn format && yarn compile",
    "bundle": "webpack",
    "bundle:watch": "webpack --watch",
    "clean": "rimraf lib",
    "compile": "tsc --build",
    "format": "yarn prettier-package-json --write package.json && yarn prettier --write .",
    "test": "echo \"These need to be re-enabled, they broke.\""
